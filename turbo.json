{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "lint-staged": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "outputs": []
    }
  }
}