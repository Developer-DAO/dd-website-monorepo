{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "lint-staged": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "outputs": []
    },
    "cms#deploy": {
      "cache": false,
      "dependsOn": [
        "^cms#build"
      ]
    },
    "frontend#test": {
      "dependsOn": []
    },
    "frontend#build": {
      "dependsOn": [
        "^cms#deploy"
      ]
    },
    "frontend#deploy": {
      "cache": false,
      "dependsOn": [
        "cms#deploy",
        "frontend#build"
      ],
      "outputs": []
    }
  }
}