{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        ".build"
      ]
    },
    "deploy": {
      "dependsOn": [
        "frontend#test"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": []
    },
    "lint-staged": {
      "outputs": []
    },
    "dev": {
      "outputs": []
    },
    "start": {},
    "start-cms": {
      "dependsOn": [
        "cms#build"
      ]
    },
    "start-frontend": {
      "dependsOn": [
        "start-cms",
        "frontend#build"
      ],
      "outputs": []
    },
    "frontend#build": {
      "dependsOn": [],
      "outputs": []
    }
  }
}